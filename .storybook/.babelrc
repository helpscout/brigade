{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": 11,
          "edge": 14,
          "firefox": 45,
          "chrome": 49,
          "safari": 10,
          "node": "6.11"
        }
      }
    ],
    "@babel/preset-flow",
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-flow-strip-types",
    "inline-svg",
    "emotion"
  ]
}
